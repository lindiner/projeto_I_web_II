{
  "name": "netfilx",
  "version": "1.0.0",
  "description": "Projeto I",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./index.js",
    "migrate:run": "knex --knexfile knexfile.ts --cwd src/ --env development migrate:latest ",
    "seed:run": "knex --knexfile knexfile.ts --cwd src/ --env development seed:run ",
    "migrate:rollback": "knex --knexfile knexfile.ts --cwd src/ --env development migrate:rollback --all",
    "migrate:clean": "npm run migrate:rollback && npm run migrate:run && npm run seed:run"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "knex": "^0.95.6",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
